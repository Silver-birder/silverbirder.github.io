{
  "name": "docs",
  "version": "0.0.4",
  "description": "App with Routing built-in ready to create your app",
  "packageManager": "pnpm@8.6.2",
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.2"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prebuild.pandacss": "panda codegen --silent",
    "prepare": "panda codegen",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test": "pnpm test.unit && pnpm test.e2e",
    "test.e2e": "cucumber-js",
    "test.unit": "vitest run components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=ja-JP,en-US --assetsPath=i18n",
    "gpt-translate-json": "source .env && gpt-translate-json --apiKey=$PUBLIC_OPEN_API_API_KEY --model=gpt-4 --maxTokens=3000 --langs=ja-JP,en-US --originalLang=ja-JP",
    "chromatic": "source .env && npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@builder.io/qwik": "^1.2.7",
    "@builder.io/qwik-city": "^1.2.7",
    "@builder.io/vite-plugin-macro": "~0.0.7",
    "@openreplay/tracker": "^9.0.9",
    "@openreplay/tracker-assist": "^6.0.1",
    "@pandacss/dev": "^0.8.0",
    "@storybook/addon-essentials": "7.3.1",
    "@storybook/addon-links": "7.3.1",
    "@storybook/blocks": "7.3.1",
    "@storybook/builder-vite": "7.3.1",
    "@storybook/html": "7.3.1",
    "@storybook/html-vite": "7.3.1",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.5.0",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@unpic/qwik": "^0.0.25",
    "@vitest/ui": "^0.33.0",
    "chromatic": "^7.4.0",
    "eslint": "8.44.0",
    "eslint-plugin-qwik": "^1.2.7",
    "eslint-plugin-storybook": "^0.6.13",
    "gpt-translate-json": "^0.1.0",
    "lottie-web": "^5.12.2",
    "prettier": "2.8.8",
    "qwik-speak": "^0.16.0",
    "storybook": "7.3.1",
    "storybook-framework-qwik": "0.2.3",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.12",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.33.0",
    "xml2js": "^0.6.2"
  },
  "readme": "ERROR: No README data found!",
  "_id": "docs@0.0.4"
}
